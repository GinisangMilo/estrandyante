<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>


<StackPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ReportGenerationController">
   <children>
      <AnchorPane prefHeight="566.0" prefWidth="496.0">
         <children>
            <Pane layoutX="8.0" layoutY="10.0" prefHeight="490.0" prefWidth="480.0" styleClass="pne" stylesheets="@application.css">
               <children>
                  <TextArea fx:id="reportTextArea" layoutX="14.0" layoutY="42.0" prefHeight="432.0" prefWidth="453.0" />
                  <Label layoutX="58.0" layoutY="8.0" text="Generate Report by Strand" textFill="WHITE">
                     <font>
                        <Font name="Tahoma Bold" size="27.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <Button fx:id="btnClose" layoutX="14.0" layoutY="517.0" mnemonicParsing="false" onAction="#handleClose" prefHeight="25.0" prefWidth="87.0" styleClass="btn" stylesheets="@application.css" text="Back" />
            <Button fx:id="btnGenerateReport" layoutX="128.0" layoutY="517.0" mnemonicParsing="false" onAction="#handleGenerateReport" prefHeight="36.0" prefWidth="353.0" styleClass="btn" stylesheets="@application.css" text="Generate Report" />
         </children>
      </AnchorPane>
   </children>
</StackPane>
